@model IEnumerable<DOT.NET.Models.Przedmiot>
@using DOT.NET.OwnHelpers







    @foreach (var przedmiot in Model)
    {

        
            <div class="text-center" style="display:inline-block; padding-left:20px;">
                <h5>@przedmiot.Nazwa <br />--@przedmiot.Producent--</h5>
                <img src=@Url.ObrazekSciezka(przedmiot.NazwaPlikuObrazka) class="rounded" alt="" width="100" height="100"><br />
                <a href=@Url.Action("DodajDoKoszyka", "Koszyk", new { id = przedmiot.PrzedmiotId })><img src="~/Content/Images/koszyk.png" width="20" height="20" /> </a> <a href=@Url.Action("Szczegoly", "Przemioty", new {id = przedmiot.PrzedmiotId})><img src="~/Content/Images/lupka.png" width="20" height="20" style="padding-left:5px;" /></a>
            </div>        
    }
