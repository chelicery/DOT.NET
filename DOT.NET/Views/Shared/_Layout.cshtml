<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="@Url.Content("~/")">Sklep internetowy</a>
            </div>
            <ul class="nav navbar-nav">
                <li><a href="link" style="float:right;">Koszyk</a></li>
                <!--TagHelper asp-append-version -->
                <li><img src="~/Content/Images/logo.png" asp-append-version="true"></li>
             </ul>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>

            <div style="width:50%; margin:0 auto; padding-top: 4px;">
                <!-- url.action helpers -->
                <a href="@Url.Action("StronyStatyczne", "Home", new { nazwa = "Onas" }, null)" type="button" class="btn btn-default">O nas</a>
                <a href="@Url.Action("StronyStatyczne", "Home", new { nazwa = "Regulamin" }, null)" type="button" class="btn btn-default">Regulamin</a>
                <a href="@Url.Action("StronyStatyczne", "Home", new { nazwa = "Dostawa" }, null)" type="button" class="btn btn-default">Dostawa</a>
                <a href="@Url.Action("StronyStatyczne", "Home", new { nazwa = "Kontakt" }, null)" type="button" class="btn btn-default">Kontakt</a>
            </div>
            <br />
            <p>&copy; @DateTime.Now.Year DOT </p>

        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>